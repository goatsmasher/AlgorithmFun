function BubbleSort(arr){
  for (var i = 0; i < arr.length; i++){
    if (arr[i] > arr[i+1]){
      var temp = arr[i];
      arr[i] = arr[i+1];
      arr[i+1] = temp;
      i = -1;
    }
  }
  return arr;
}

// BigO - O(n2)

function SelectionSort(arr){
  for (var i = 0; i < arr.length; i++){
    for (var k = i+1; k < arr.length; k++){
      if (arr[k] < arr[i]){
        var temp = arr[k];
        arr[k] = arr[i];
        arr[i] = temp;
      }
    }
  }
  return arr;
}

// BigO - O(n2)
